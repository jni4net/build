<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <description>Maven Plugin for .NET: Handles source and test compiles</description>
  <groupId>org.sonatype.nmaven.plugins</groupId>
  <artifactId>maven-dotnet-compiler-plugin</artifactId>
  <version>0.17.jni4net.0</version>
  <goalPrefix>dotnet-compiler</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>process-test-sources</goal>
      <description>Copies test source files to target directory.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>process-test-sources</phase>
      <implementation>org.sonatype.nmaven.plugin.compiler.TestSourceProcessorMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>excludes</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>includes</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>language</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>.NET Language. The default value is &lt;code&gt;C_SHARP&lt;/code&gt;. Not case or white-space sensitive.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>true</editable>
          <description>The maven project.</description>
        </parameter>
      </parameters>
      <configuration>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <excludes implementation="java.lang.String[]">${excludes}</excludes>
        <language implementation="java.lang.String" default-value="C_SHARP">${language}</language>
        <includes implementation="java.lang.String[]">${includes}</includes>
      </configuration>
    </mojo>
    <mojo>
      <goal>process-sources</goal>
      <description>Copies source files to target directory.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>process-sources</phase>
      <implementation>org.sonatype.nmaven.plugin.compiler.SourceProcessorMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>excludes</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>includes</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>language</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>.NET Language. The default value is &lt;code&gt;C_SHARP&lt;/code&gt;. Not case or white-space sensitive.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>true</editable>
          <description>The maven project.</description>
        </parameter>
      </parameters>
      <configuration>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <excludes implementation="java.lang.String[]">${excludes}</excludes>
        <language implementation="java.lang.String" default-value="C_SHARP">${language}</language>
        <includes implementation="java.lang.String[]">${includes}</includes>
      </configuration>
    </mojo>
    <mojo>
      <goal>test-compile</goal>
      <description>Maven Mojo for compiling Class files to the .NET Intermediate Language.</description>
      <requiresDependencyResolution>test</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>test-compile</phase>
      <implementation>org.sonatype.nmaven.plugin.compiler.TestCompilerMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>frameworkVersion</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The framework version to compile under: 1.1, 2.0, 3.0</description>
        </parameter>
        <parameter>
          <name>language</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>.NET Language. The default value is &lt;code&gt;C_SHARP&lt;/code&gt;. Not case or white-space sensitive.</description>
        </parameter>
        <parameter>
          <name>localRepository</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The location of the local Maven repository.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>true</editable>
          <description>The maven project.</description>
        </parameter>
        <parameter>
          <name>vendorName</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The Vendor for the Compiler. Not case or white-space sensitive.</description>
        </parameter>
      </parameters>
      <configuration>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <vendorName implementation="java.lang.String">${vendor}</vendorName>
        <frameworkVersion implementation="java.lang.String" default-value="2.0.50727">${frameworkVersion}</frameworkVersion>
        <language implementation="java.lang.String" default-value="C_SHARP">${language}</language>
        <localRepository implementation="java.io.File">${settings.localRepository}</localRepository>
      </configuration>
      <requirements>
        <requirement>
          <role>org.sonatype.nmaven.compiler.DotnetCompilerContext</role>
          <field-name>compilerContext</field-name>
        </requirement>
      </requirements>
    </mojo>
    <mojo>
      <goal>compile</goal>
      <description>Maven Mojo for compiling Class files to the .NET Intermediate Language.</description>
      <requiresDependencyResolution>compile</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>compile</phase>
      <implementation>org.sonatype.nmaven.plugin.compiler.CompilerMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>aditionalOptions</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>More compiler options</description>
        </parameter>
        <parameter>
          <name>frameworkVersion</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The framework version to compile under: 1.1, 2.0, 3.0</description>
        </parameter>
        <parameter>
          <name>keycontainer</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Specifies a strong name key container.</description>
        </parameter>
        <parameter>
          <name>keyfile</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>Specify a strong name key file.</description>
        </parameter>
        <parameter>
          <name>language</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>.NET Language. The default value is &lt;code&gt;C_SHARP&lt;/code&gt;.</description>
        </parameter>
        <parameter>
          <name>localRepository</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The location of the local Maven repository.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>true</editable>
          <description>The maven project.</description>
        </parameter>
        <parameter>
          <name>vendorName</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The Vendor for the Compiler.</description>
        </parameter>
      </parameters>
      <configuration>
        <keycontainer implementation="java.lang.String">${keycontainer}</keycontainer>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <keyfile implementation="java.io.File">${keyfile}</keyfile>
        <vendorName implementation="java.lang.String">${vendor}</vendorName>
        <frameworkVersion implementation="java.lang.String" default-value="2.0.50727">${frameworkVersion}</frameworkVersion>
        <aditionalOptions implementation="java.lang.String">${aditionalOptions}</aditionalOptions>
        <language implementation="java.lang.String" default-value="C_SHARP">${language}</language>
        <localRepository implementation="java.io.File">${settings.localRepository}</localRepository>
      </configuration>
      <requirements>
        <requirement>
          <role>org.sonatype.nmaven.compiler.DotnetCompilerContext</role>
          <field-name>dotnetCompilerContext</field-name>
        </requirement>
      </requirements>
    </mojo>
    <mojo>
      <goal>generate-assembly-info</goal>
      <description>Generates an AssemblyInfo.* class based on information within the pom file.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>generate-sources</phase>
      <implementation>org.sonatype.nmaven.plugin.compiler.AssemblyInfoGeneratorMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>isCLSCompliant</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>language</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>.NET Language. The default value is &lt;code&gt;C_SHARP&lt;/code&gt;. Not case or white-space sensitive.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>The maven project.</description>
        </parameter>
        <parameter>
          <name>sourceDirectory</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Source directory containing the copied class files.</description>
        </parameter>
      </parameters>
      <configuration>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <isCLSCompliant implementation="boolean" default-value="false">${CLSCompliant}</isCLSCompliant>
        <language implementation="java.lang.String" default-value="C_SHARP">${language}</language>
        <sourceDirectory implementation="java.lang.String" default-value="${project.build.sourceDirectory}">${sourceDirectory}</sourceDirectory>
      </configuration>
      <requirements>
        <requirement>
          <role>org.sonatype.nmaven.assembler.AssemblerContext</role>
          <field-name>assemblerContext</field-name>
        </requirement>
      </requirements>
    </mojo>
  </mojos>
